{
	"name": "Example",
	"version": "0.1.0",
	"author": [
		"Jane Doe",
		"George Foo",
		"..."
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Example",
	"descriptionmsg": "example-desc",
	"type": "other",
	"SpecialPages": {
		"HelloWorld": "SpecialHelloWorld"
	},
	"APIListModules": {
		"example": "ApiQueryExample"
	},
	"MessagesDirs": {
		"Example": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"ExampleAlias": "Example.i18n.alias.php",
		"ExampleMagic": "Example.i18n.magic.php"
	},
	"AutoloadClasses": {
		"ExampleHooks": "Example.hooks.php",
		"SpecialHelloWorld": "specials/SpecialHelloWorld.php",
		"ApiQueryExample": "api/ApiQueryExample.php"
	},
	"ResourceModules": {
		"ext.Example.welcome": {
			"scripts": [
				"modules/ext.Example.welcome.js"
			],
			"styles": [
				"modules/ext.Example.welcome.css"
			],
			"messages": [
				"example-welcome-title-loggedout",
				"example-welcome-title-user",
				"example-welcome-color-label",
				"example-welcome-color-value"
			],
			"dependencies": [
				"mediawiki.util",
				"mediawiki.user",
				"mediawiki.Title"
			]
		},
		"ext.Example.welcome.init": {
			"scripts": "modules/ext.Example.welcome.init.js",
			"dependencies": [
				"ext.Example.welcome"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "examples/Example"
	},
	"Hooks": {
		"BeforePageDisplay": "ExampleHooks::onBeforePageDisplay",
		"ResourceLoaderGetConfigVars": "ExampleHooks::onResourceLoaderGetConfigVars",
		"ParserFirstCallInit": "ExampleHooks::onParserFirstCallInit",
		"MagicWordwgVariableIDs": "ExampleHooks::onRegisterMagicWords",
		"ParserGetVariableValueSwitch": "ExampleHooks::onParserGetVariableValueSwitch",
		"LoadExtensionSchemaUpdates": "ExampleHooks::onLoadExtensionSchemaUpdates"
	},
	"config": {
		"ExampleEnableWelcome": true,
		"ExampleWelcomeColorDefault": "#eee",
		"ExampleWelcomeColorDays": {
			"Monday": "orange",
			"Tuesday": "blue",
			"Wednesday": "green",
			"Thursday": "red",
			"Friday": "yellow"
		},
		"ExampleMyWord": "Awesome"
	},
	"manifest_version": 1
}
